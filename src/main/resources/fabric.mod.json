{
	"schemaVersion": 1,
	"id": "custom-stuff",
	"version": "${version}",
	"name": "Custom Stuff",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"cillow"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/custom-stuff/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
          "com.cillow.customstuff.CustomStuff"
		],
		"client" : [
			"com.cillow.customstuff.CustomStuffClient"
		],
		"fabric-datagen": [
			"com.cillow.customstuff.CustomStuffDataGenerator"
		]
	},
	"mixins": [
		"custom-stuff.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.20.1",
		"java": ">=17",
